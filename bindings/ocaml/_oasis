Library keystone
  Path: .
  Modules: Keystone	
  InternalModules: Ffi_generated, Ffi_generated_types
  CSources: ffi_generated_stubs.c
  CCLib: -lkeystone
  CCOpt:            -I $pkg_ctypes_stubs
  BuildDepends:     keystone_bindings, ctypes
  CompiledObject: native

